@using asc_general.Models;
@{
    var u = Session["user"] as user;
}

<!DOCTYPE HTML>

<html lang="az" class="no-js">

<head>
    <meta http-equiv="content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>Junior.az</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="shortcut icon" href="~/Public/images/mypics/babyfoot.ico" type="image/x-icon" />
    <link rel="stylesheet" href="~/Public/bootstrap/css/bootstrap.css">

    @RenderSection("herflerCss", false)
    @RenderSection("adlarCss", false)
    @RenderSection("tehsilCss",false)
    @RenderSection("nameCss",false)
    @RenderSection("cizgiCss",false)
    @RenderSection("foodblogCss", false)
    @RenderSection("idmanCss", false)
    @RenderSection("foodCss", false)
    @RenderSection("healthCss", false)
    @RenderSection("homeCSs", false)
    @RenderSection("dr_profileCss", false)
    @RenderSection("complexCss", false)
    @RenderSection("receptCss", false)
    @RenderSection("allComplexCss",false)
    @RenderSection("searchCss",false)
    @RenderSection("allReceptsCss", false)
    

    <link href="~/Public/style.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/shortcodes.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/responsive.css" rel="stylesheet" type="text/css">
    <link href="~/Public/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!--jquery-->
   
</head>


<body class="main" style="background:white;">
    <!--wrapper starts-->
    <div class="wrapper">
        <!--header starts-->
        <header>
            <div class="container">
                <div class="logo">
                    <a href="index.html" title="Kids Life">
                        <img src="~/Public/images/logojunior.png" alt="kids life" title="kids life" style="width:135px; margin-top:14px;">
             
                    </a>
                </div>
                @{

                if (Session["user"] == null)
                {
                    <div class="login-details">
                        <p class="login">
                            <a href="@Url.Action("Register", "user")">Qeydiyyat <i class="fa fa-user login_icon" aria-hidden="true"></i></a>
                            <a href="@Url.Action("Login", "user")">Giriş <i class="fa fa-sign-in login_icon" aria-hidden="true"></i></a>
                        </p>
                    </div>
                    }
                    else
                    {
                        <div class="login-details">
                            <p class="login">
                                <a href="@Url.Action("index", "user")" style="color:white; margin-left:20px;"><i class="fa fa-user-circle-o" aria-hidden="true" style="margin-left:5px; font-size: 20px;"></i> @u.username</a>
                            </p>
                            </div>
                            }

                            }



                        </div>
            <!--menu-container starts-->
            <div id="menu-container">
                <div class="container">
                    <!--nav starts-->
                    <nav id="main-menu">
                        <div class="dt-menu-toggle" id="dt-menu-toggle">Menu<span class="dt-menu-toggle-icon"></span></div>
                        <ul id="menu-main-menu" class="menu">
                            <li class="current_page_item @ViewBag.home">
                                @Html.ActionLink("Ana Səhifə", "Index", "Home")
                            </li>
                            <li class="mustard @ViewBag.letter">
                                @Html.ActionLink("Adlar", "Index", "Letters")
                            </li>
                            <li class="yellow @ViewBag.cartoon">
                               @Html.ActionLink("Cizgi Filmlər", "Index", "Cartoon")
                            </li>
                            <li class="blue @ViewBag.gym">
                               @Html.ActionLink("İdman", "Index", "gym")
                            </li>
                            <li class="steelblue @ViewBag.food">
                                @Html.ActionLink("Qida", "Index", "Food")
                            </li>
                            <li class="lavender @ViewBag.education">
                                @Html.ActionLink("Təhsil", "Index", "Education")
                            </li>
                            <li class="purple @ViewBag.health">
                                @Html.ActionLink("Sağlamlıq", "Index", "Health")
                            </li>
                        </ul>
                    </nav>
                    <!--nav ends-->

                    <ul id="icons" class="dt-sc-social-icons">
                        <li>
                            <a href="#" title="Facebook" class="dt-sc-tooltip-top facebook">
                                <span class="fa fa-facebook"></span>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Twitter" class="dt-sc-tooltip-top twitter">
                                <span class="fa fa-twitter"></span>
                            </a>
                        </li>
                        <li>
                            <a href="#" title="Google Plus" class="dt-sc-tooltip-top gplus">
                                <span class="fa fa-google-plus"></span>
                            </a>
                        </li>
                        <li>
                            <a class="dt-sc-tooltip-top search">
                                <span class="fa fa-search"></span>
                            </a>
                        </li>
                    </ul>

                    <form action="@Url.Action("Index", "Search", new { id = 1 })" method="get" class="formSearch" autocomplete="off">
                        <div class="search_tool">
                            <input type="text" placeholder="Axtar" id="searchString" name="searchString">
                            <button type="submit" value="Submit">
                                <span class="fa fa-search"></span>
                            </button>
                            <ul id="searchingResult" class="searchingResult"></ul>
                        </div>
                    </form>
                </div>
            </div>
            <!--menu-container ends-->
        </header>
        <!--header ends-->
        @RenderBody()
        <!--footer starts-->
        <footer>
            <!--footer-widgets-wrapper starts-->
            <div class="footer-widgets-wrapper">
                <!--container starts-->
                <div class="container">
                    <div id="main">
                        <aside class="widget widget_text">
                            <h3 class="dt-sc-hr-green-title">Kontakt</h3>
                            <div class="row contact">
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-map-marker" aria-hidden="true"></i>
                                        <p>Nizami küç. 203B, AF Business House, 2-ci mərtəbə</p>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-envelope" aria-hidden="true"></i>
                                        <p>Code.edu.az</p>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <div class="contact_info">
                                        <i class="fa fa-phone" aria-hidden="true"></i>
                                        <p>+994 70 333 27 77</p>
                                    </div>
                                </div>
                            </div>
                        </aside>
                    </div>
                </div>
                <!--container ends-->
            </div>
            <!--footer-widgets-wrapper ends-->
            <div class="copyright">
                <div class="container">
                    <p class="copyright-info">© 2014 Kids Life . All rights reserved.</p>
                    <div class="footer-links">
                        <p>Follow us</p>
                        <ul class="dt-sc-social-icons">
                            <li class="facebook">
                                <a href="#"><img src="~/Public/images/facebook.png" alt="" title=""></a>
                            </li>
                            <li class="twitter">
                                <a href="#"><img src="~/Public/images/twitter.png" alt="" title=""></a>
                            </li>
                            <li class="gplus">
                                <a href="#"><img src="~/Public/images/gplus.png" alt="" title=""></a>
                            </li>
                            <li class="pinterest">
                                <a href="#"><img src="~/Public/images/pinterest.png" alt="" title=""></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
        <!--footer ends-->
    </div>
    <!--wrapper ends-->
    <a href="#" title="Go to Top" class="back-to-top">To Top ↑</a>

    @RenderSection("tehsilJS",false)
    @RenderSection("foodblogJs",false)
    @RenderSection("homeJs", false)
    @RenderSection("carouselJS",false)
    @RenderSection("healthJs",false)
    @RenderSection("receptcarouselJS", false)
    

    <!--Java Scripts--> 
   
 

    <!--Java Scripts-->
    <script type="text/javascript" src="~/Public/js/jquery.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery-migrate.min.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.validate.min.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery-easing-1.3.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.sticky.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.nicescroll.min.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.inview.js"></script>
    <script type="text/javascript" src="~/Public/js/validation.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.tipTip.minified.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.prettyPhoto.js"></script>
    <script type="text/javascript" src="~/Public/js/twitter/jquery.tweet.min.js"></script>
    <script type="text/javascript" src="~/Public/js/jquery.parallax-1.1.3.js"></script>
    <script type="text/javascript" src="~/Public/js/shortcodes.js"></script>
    <script type="text/javascript" src="~/Public/js/custom.js"></script>
    <script type="text/javascript" src="~/Public/bootstrap/js/bootstrap.js"></script>
    <script type="text/javascript" src="~/Public/js/senem.js"></script>
    <script type="text/javascript" src="~/Public/js/search.js"></script>

    <!-- Layer Slider -->
    <script type="text/javascript" src="~/Public/js/jquery-transit-modified.js"></script>
    <script type="text/javascript" src="~/Public/js/layerslider.kreaturamedia.jquery.js"></script>
    <script type='text/javascript' src="~/Public/js/greensock.js"></script>
    <script type='text/javascript' src="~/Public/js/layerslider.transitions.js"></script>
    <script type="text/javascript">var lsjQuery = jQuery;</script>
    <script type="text/javascript">
     

        var lsjQuery = jQuery;
    </script>
